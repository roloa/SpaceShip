,"


function zakoInit(){
	if(!this.level){
		this.level = 100;
	}
	
	this.vx = 0;
	this.vy = 0;
	
	this.power = 1; //仮の値
	this.life = 1;
	
	this.isUnit = true;
	this.isEnemy = true;
	
}

//死亡時の処理
//アイテムドロップとか
function onDeath(){
	this._parent.attach(""bomb"",{_x:this._x,_y:this._y});
	
//	TODO ドロップアイテムの個別設定
	this._parent.attach(""coin"",{_x:this._x,_y:this._y}); 
	this._parent.attach(""fragment"",{_x:this._x,_y:this._y}); 
//	this._parent.attach(""coin"",{_x:this._x,_y:this._y});
//	this._parent.attach(""coin"",{_x:this._x,_y:this._y});
	if(Math.random() < 0.1){this._parent.attach(""missileBox"" , {_x:this._x,_y:this._y});}
}

function restrictSpeed(){
	this.vx = Math.min(Math.max(this.vx , -3) , 3);
	this.vy = Math.min(Math.max(this.vy , -3) , 3);
}
//何かにぶつかられた時、ぶつかってきた相手が引数になって呼ばれる
function onHit(oppo){

	//onHitplus();
	
	//this.vx += 1;
	//this.vy =  -this.vy;
	if(oppo.vx) this.vx += oppo.vx * 0.5;
	if(oppo.vy) this.vy += oppo.vy * 0.5;
	this.restrictSpeed();
	
	//相手のパワーの分ライフが減って、なくなった場合は死ぬ
	this.onDamage(oppo.power);
}

function onDamage(amount){
	this._parent.attach(""dmgPart"",{_x:this._x,_y:this._y , dmg: amount});
	
	this.life -= amount;
	if(this.life <= 0){
		this.onDeath();
		this._parent.remove(this,this.isEnemy);
	}
}
function zakoPhysical(){
	//画面外ロスト判定
	//敵なのでちょっと広い
	if(	this._x < 0 || 1400 < this._x ||
		this._y < -100 || 740 < this._y){
		this._parent.remove(this,this.isEnemy);
	} else if(Math.random() < 0.1){
		this._parent.ship.setHate(this);
	}
	
		
	//直接衝突当たり判定
	var hitter = this._parent.hitCheck(this._x,this._y,this.isEnemy);
	if(hitter){
		//後ろにノックバック
		this.vx = 5;
		//this._x += 1 + this.vx;
		this.restrictSpeed();
		
		//向こうにダメージをあたえる
		hitter.onHit(this);
		//自分もダメージを受ける
		this.onDamage(this.power);

		//4方向に弾を発射して消える
		//_parent.attach(""Eshot"",{_x:_x,_y:_y,_rotation:45,power:power});
		//_parent.attach(""Eshot"",{_x:_x,_y:_y,_rotation:-45,power:power});
		//_parent.attach(""Eshot"",{_x:_x,_y:_y,_rotation:135,power:power});
		//_parent.attach(""Eshot"",{_x:_x,_y:_y,_rotation:-135,power:power});
		//this._parent.remove(this,isEnemy);
	}
	
}

