	enemys = new Array();
	allys = new Array();
	enemyIndex = 0;
	allyIndex = 0;
	enemyNum = 0;
	allyNum = 0;
	MAX_OBJ = 10;
	//当たり判定リストに登録する。
	//登録オブジェクトの数がMAX_OBJを超えた場合は無視する

	//登録を解除する。
	function remove(mc,isEnemy){
		if(isEnemy){
			enemyNum -= 1;
			enemys[mc.registNum] = null;
		} else {
			allyNum -= 1;
			allys[mc.registNum] = null;
		}
		mc.removeMovieClip();
	}
	
	function spawnZako(spawnLevel){
		//ランダムにザコ敵を生成する
		//TODO  生成される敵は、starmap.starsのtypeに影響される
		//TODO radiusが大きいほど影響度が強い
		
		if(station.isAlive){
			return;
		}
		//敵の生成されるタイミング
		//前に進んでいるときのランダム湧き、アンテナで促進
		attach(""zako"",{
			_x:1400,
			_y:100 + Math.random() * 400,
			level:spawnLevel},
			true,true);
		//ボスが出たときの固有の取り巻き、には使わないで行こうかな
	}
	
	//指定座標にある敵対飛翔物を返す。
	//静的に配置されているものもチェックする。
	//isEnemyのところは、自分の属性を指定する。
	function hitCheck(x,y,isEnemy){
		if(isEnemy){
			if( shield.hitTest(x,y,true)) return shield;
			if( tate.myHit(x,y)) return tate;
			if( ship.hitTest(x,y,true)) return ship;
			if( satelite.hitTest(x,y,true)) return satelite;
			if( roloa.hitTest(x,y,true)) return roloa;
			for(var i=0 ; i < MAX_OBJ ; i++){
				if(allys[i].hitTest(x,y,true)){
					return allys[i];
				}
			}
		} else {
			if( enemy.hitTest(x,y,true)) return enemy;
			
			for(var i=0 ; i < MAX_OBJ ; i++){
				if(enemys[i].hitTest(x,y,true)){
					return enemys[i];
				}
			}
		}
		return null;
	}
	
	prevTime = 0;
	prevRestTime = 0;
	
	
/**
	オブジェクトの作成を行う。
	name : 作るオブジェクトの書き出し識別子
	obj  : 初期化用オブジェクト
	isHitobj : 当たり判定を持つオブジェクトかどうか
	isEnemy : 所属勢力
*/
function attach(name,obj,isHitobj,isEnemy){

	if(isHitobj){
		if(isEnemy){
			if(enemyNum < MAX_OBJ){
				var sindo = this.getNextHighestDepth();
				var newobj = this.attachMovie(name,name + sindo,sindo,obj);
				enemyNum += 1;
				do {
					enemyIndex++; 
					if(MAX_OBJ <= enemyIndex){
						enemyIndex = 0;
					}
				} while(enemys[enemyIndex] != null);
				enemys[enemyIndex] = newobj;
				newobj.registNum = enemyIndex;
				
			} else { trace(""enemyNum is Over!""); }
		} else {
			if(allyNum < MAX_OBJ){
				var sindo = this.getNextHighestDepth();
				var newobj = this.attachMovie(name,name + sindo,sindo,obj);
				allyNum += 1;
				do {
					allyIndex++; 
					if(MAX_OBJ <= allyIndex){
						allyIndex = 0;
					}
				} while(allys[allyIndex] != null);
				allys[allyIndex] = newobj;
				newobj.registNum = allyIndex;
				//trace(""ally regist at "" + allyIndex);
			} else { trace(""allyNum is Over!""); }
		}
	} else {
		var sindo = this.getNextHighestDepth();
		this.attachMovie(name,name + sindo,sindo,obj);
	}
}
