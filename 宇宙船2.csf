function pressButton(num){
	
	if(selectedNum == num + buttonScroll){
		//前回と同じ武器をクリックした場合
		ON_OFF();
	} else {
		selectedNum = num + buttonScroll;
		reloadDisp();
	}
}

function ON_OFF(){
	if( weapons[selectedNum].level == 0 ){
		//ロックされている武器をオンオフしようとしたとき
		//lvup();
	} else {
		weapons[selectedNum].ON_OFF();
		reloadDisp();
	}
}

function lvup(){
	if(weapons[selectedNum]){
		if( _root.main.ship.useMetal( weapons[selectedNum].getCost() )){
			weapons[selectedNum].level += 1;
		}
		reloadDisp();
	}
}

//ボタン表示を更新する
function reloadDisp(){
	//アイテム表示
	for(var i=0 ; i < 6 ; i++){
		if(weapons[i + buttonScroll] == undefined){
			buttons[i].gotoAndStop( 1 );
			buttons[i].bgEnable._visible = false;
			buttons[i].lock._visible = false;
		} else {
			buttons[i].gotoAndStop( i + buttonScroll );
			//オンオフ表示
			buttons[i].bgEnable._visible = ( weapons[i + buttonScroll].isEnable );
			buttons[i].lock._visible = ( weapons[i + buttonScroll].level == 0 );
		}
	}
	
	
	//選択枠表示
	var cursorNum = selectedNum - buttonScroll;
	if(0 <= cursorNum && cursorNum < 6){
		cursor._x = buttons[cursorNum]._x;
		cursor._y = buttons[cursorNum]._y;
		cursor._visible = true;
	} else {
		//cursor._alpha = 50;
		cursor._visible = false;
	}
	
	//テキスト類
	if(weapons[selectedNum ] == undefined){
		weaponName.text = ""無し"";
		setsumei.text = ""（武器は）ないです"";
		level.text = ""?"";
		cost.text = ""?"";
	} else {
		weaponName.text = weapons[selectedNum ].name;
		setsumei.text = weapons[selectedNum   ].setsumei;
		level.text = weapons[selectedNum      ].level;
		cost.text = weapons[selectedNum       ].getCost();
	}
}

