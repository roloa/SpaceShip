1,1,False,False,True,,"function dropUpgrade(upgrade){
	//アップグレードをドロップする関数
	var x = upgrade._x;
	var y = upgrade._y;
	//各アイコンと当たり判定をとる
	if(roloa.hitTest( x , y )) _parent.roloa.upgrade(upgrade);
	else if(satelite.hitTest( x , y )) _parent.satelite.upgrade(upgrade);
	else if(ship.hitTest( x , y )) _parent.ship.upgrade(upgrade);
