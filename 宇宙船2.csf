setRandom();

function setRandom(){
	//ランダムな取引を設定する
	/*
		リソースの基本価値
		1コイン = 2ミサイル = 4メタル
		
		1ミサイル消費->1メタル獲得
		
		取引元
		所持リソースの99% ~ 9%
		
		取引先
		価値レート × 100% ~ 50%
	*/
	
	//たくさん持っている物が選ばれやすい
	var c = check(0);
	var ms = check(1);
	var mt = check(2);
	var rnd = (c + ms + mt) * Math.random();
	if(rnd < c){
		sellKind = 0;
	} else if(rnd < c + ms){
		sellKind = 1;
	} else {
		sellKind = 2;
	}
	
	//sellKind = Math.floor(Math.random() * 3);
	buyKind = (sellKind + 1 + Math.floor(Math.random() * 2)) % 3;
	
	sellAmount = Math.floor(check(sellKind) * 
		((Math.random() * 30) + (Math.random() * 30) + (Math.random() * 30) + 9) / 100 ) ;
	
	buyAmount = Math.floor( sellAmount * 
	 ((Math.random() * 25) + (Math.random() * 25) + 49)/ 100);
	
	sellIcon.gotoAndStop(sellKind + 2);
	buyIcon.gotoAndStop(buyKind + 2);
	sell.text = sellAmount;
	buy.text = buyAmount;
	
	_visible = true;
}
function check(kind){
	switch( kind ){
		case 0:
			return _root.main.ship.resCoin;
		case 1:
			return _root.main.ship.resMissile;
		case 2:
			return _root.main.ship.resMetal;
	}
}
function pay(kind , amount){
	//払う
	switch( kind ){
		case 0:
			return _root.main.ship.useCoin( amount );
		case 1:
			return _root.main.ship.useMissile( amount );
		case 2:
			return _root.main.ship.useMetal( amount );
	}
}


function onRelease(){
	//if(_parent._visible){ //フキダシの可視状態に依存
	trace(sellAmount);
		if( pay(sellKind , sellAmount) ){
		trace(buyAmount);
			pay(buyKind , -buyAmount);
			_visible = false;

		}
