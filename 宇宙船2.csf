		carryItem.user = null;
		carryItem.vx += -_xscale * 0.01;
		carryItem.vy += -1;
		carryItem = null;
	}
	
	function onHit(obj){
		vx += obj.vx;
		vy += obj.vy;
		vx = Math.min(Math.max(vx,-5),5);
		vy = Math.min(Math.max(vy,-5),5);
		life -= obj.power;
		if(life <= 0){
			isDead = true;
			life = 0;
			resetWeapon();
			_root.UI.msg.Q(""サテライトがやられました。"");

		}
	}
	function upgrade(item){
		//1:リペア 2:ライフ 3:パワー 4:リロード 5:マガジン
		switch(item.kind){
			case 1: life = maxLife; break;
			case 2: maxLife += 50; life += 50; break;
			case 3: power += 1; break;
			case 4: reloadTime *= 0.8; break;
			case 5: magagineSize += 1; break;
		}
	}
	function getStats(){
		return
		""Power: "" + power +
		"" Reload:"" + reloadTime  +""\n"" +
		""Life:"" + maxLife +
		"" Magagine:"" + magagineSize;
	}
	function toggleMode(){
		resetWeapon();
		if(isResting && !isDead){
			isResting = false;
			_visible = true;
			_parent.ship.restingSatelite._visible = false;
			vy -= 1;
			if(_parent.roloa.mode == 3){
				launch();
			}
		} else if(isInside ){
			launch();
		} else if(isStayRoof){
			isStayRoof = false;
			goRoof = false;
			goInside = true;
			Lleg._rotation = -45;
			Rleg._rotation = -45;
		} else {
			goRoof = true;
		}
	}
	function launch(){
		isInside = false;
		goRoof = false;
		isStayRoof = false;
		isResting = false;
		_parent.ship.restingSatelite._visible = false;
		Larm.gotoAndStop(1);
		Rarm.gotoAndStop(1);
		_visible = true;
		Lleg._rotation = -45;
		Rleg._rotation = -45;
		_xscale = -100;
	}
	function resetWeapon(){
		//攻撃状況関係をリセットする
		_parent.satebeam._visible = false;
		beam_ammo = 0;
		beam_cool = 0;
		Larm_rot = 10;
		Rarm_rot = -10;
	}
	
	function fighting(){
		if(0 < beam_ammo){ //ビームの残弾があるならビーム発射
			//両腕を前に出して、魔法陣を展開する
			Larm_rot = 0;
			Rarm_rot = 180;
			_parent.satebeam.circle.circle._rotation += 5;
			var pt = {x:-80,y:0};
			this.localToGlobal(pt);
			_parent.satebeam._x = pt.x;
			_parent.satebeam._y = pt.y;
			if(beam_cool <= 0){
				//照射中
				_parent.satebeam._rotation = 180+_rotation;
				_parent.satebeam._visible = true;
				_parent.satebeam.circle._yscale = 5 * (beam_ammo / 3);
				_parent.satebeam.circle._xscale = _parent.satebeam.circle._yscale * 0.5;
				_parent.satebeam.beam._xscale = 1400;
				_parent.satebeam.beam._yscale = _parent.satebeam.circle._yscale;
				if(Math.random()<0.3)_parent.attach(""spark"",{_x:pt.x,_y:pt.y});
				//ビームの当たり判定
				var rad = _rotation * Math.PI / 180;
				var dx = Math.cos(rad) * 200;
				var dy = Math.sin(rad) * 200;
				var rnd = Math.random();
				var x = pt.x + dx * rnd;
				var y = pt.y + dy * rnd;
				for(var i=0 ; i <= 7 ; i++){
					var hit = _parent.hitCheck(x,y);
					if(hit){
						hit.onHit({vx:0,vy:0,power: 1 });
						_parent.attach(""spark"",{_x:x,_y:y});
						break;
					}
					x += dx;
					y += dy;
				}
				
				if(--beam_ammo <= 0){
					//終了処理
					_parent.satebeam._visible = false;
					Larm_rot = 10;
					Rarm_rot = -10;
				}
			} else {
				beam_cool -= 1;
				if(beam_cool == 0){
					soundObj.start();
				}
			}
		} else if(0 < gun_ammo){ //機銃の弾があるなら発射
			if(--gun_cool <= 0){
				var p = _parent.ship.gun.level * 3; //こっそり副砲のレベルを参照
				
				var pt = {x:-30,y:0};
				this.localToGlobal(pt);
				_parent.attach(""shot"",{
				_x:pt.x,
				_y:pt.y,
				_rotation:_rotation,
				power: p });
				
				//反動とかクールタイムとか
				_rotation -= 5 * Math.random();
				var rad = _rotation * Math.PI / 180;
				vx += Math.cos(rad) * -1;
				vy += Math.sin(rad) * -1;
				gun_cool = 10;
				gun_ammo -= 1;
			}
		} else if(--beam_reload <= 0){ //ビームのリロード完了
			beam_ammo = 60;
			beam_cool = 20;
			beam_reload = reloadTime * 3; //あとでビーム用ステータスも作る...
		} else if(--gun_reload <= 0){ //機銃のリロード完了
 			gun_ammo = 3;
			gun_reload = reloadTime;
		} else if( carryItem ){//アイテムを所持
			if(--item_interval <= 0){
				carryItem.interact();
				item_interval = 30;
			}
		}
		
	}
	function turnTo(target){
	turnToZahyo(target._x + target.vx , target._y + target.vy);

	}
	function turnToZahyo(x,y){
		var deg = Math.atan2(y - _y, x  - _x) / Math.PI * 180;
	
		_rotation = _rotation +
			(deg) * 0.1;
		Larm_rot = -deg + _rotation - 10;

	}
	

	function hitCoin(x,y){
		return (!isInside && hitTest(x,y,false));
	}
	
}

onClipEvent(enterFrame){

	_x += vx;
	_y += vy;
	
	//ポーズ修正
	Larm._rotation = Larm._rotation * 0.9 + Larm_rot * 0.1;
	Rarm._rotation = Rarm._rotation * 0.9 + Rarm_rot * 0.1;
	_rotation *= 0.9;


	//運んでいるアイテムについて
	if(carryItem){
		carryItem.vx = vx;
		carryItem.vy = vy;
		carryItem._xscale = _xscale;
		//if(mode != 1){
		carryItem._rotation = (-_rotation + Larm._rotation + 5) * _xscale / 115;
		//} else {
		//	carryItem._rotation = 0 ;
		//}
		var pt = {x:-25,y:-10};
		this.Larm.localToGlobal(pt);
		
		carryItem._x = pt.x;
		carryItem._y = pt.y;
		
		carryItem._visible = _visible;

		//var rad = (-_rotation + carryItem._rotation + 5) * Math.PI / 180;
		//carryItem._x = _x - 0 - Math.cos(rad) * 30 * _xscale / 100; //腕の長さ
		//carryItem._y = _y - 10 - Math.sin(rad) * 30 * _xscale / 100;
		//	Rarm._rotation = Larm._rotation;
	}

	if(_parent.roloa.mode == 4){
		//ロロア転落
		launch();
		
		var ydist = _parent.roloa._y - _y - 100;
		var xdist = _parent.roloa._x - _x;
		vy = (ydist) * 0.05 + 5;
		vx = (xdist) * 0.05;
		if(xdist * xdist + ydist * ydist < 1500){
			_parent.roloa.mode = 5;
		}
	} else if(_parent.roloa.mode == 5){
		//ロロアを船の上に落とす
		var ydist = _parent.ship._y - 300 - _y;
		var xdist = _parent.ship._x - _x;
		vy = (ydist) * 0.05;
		vx = (xdist) * 0.05;
		if(xdist * xdist + ydist * ydist < 2000){
			_parent.roloa.vx = 0;
			_parent.roloa.vy = 0;
			_parent.roloa.mode = 2;
		}
	} else if( _parent.roloa.mode == 7 ){
		//肩車のマニュアル操作中
		if(Key.isDown(68) || Key.isDown(Key.RIGHT)){
			vx += 0.5;
		}
		if(Key.isDown(65) || Key.isDown(Key.LEFT)){
			vx -= 0.5;
		}
		if(Key.isDown(83) || Key.isDown(Key.DOWN)){
			vy += 0.5;
		}
		if(Key.isDown(87) || Key.isDown(Key.UP)){
			vy -= 0.5;
		}
		if(Key.isDown(Key.SPACE)){
			_parent.roloa.vy = -7;
			_parent.roloa.mode = 2;
			_parent.roloa._rotation = 0;

		}
		vx *= 0.9;
		vy *= 0.9;
		fighting();
		turnToZahyo(_parent._xmouse , _parent._ymouse);
		
		//宇宙船と同じ信仰処理
		//画面外に出ないように＆背景に移動情報を送る
		if(this._x < 100){
			this.vx += 0.3;
			_parent.starmap.vx -= 1;
		}
		if(1100 < this._x){
			this.vx -= 0.3;
			_parent.starmap.vx += 1;
		}
		if(this._y < 40){
			this.vy += 0.3;
			_parent.starmap.vy -= 1;
		}
		if(600 < this._y){
			this.vy -= 0.3;
			_parent.starmap.vy += 1;
	}
			
	} else if( isResting){
		//休み中
		vx = 0;
		vy = 0;
		_x = _parent.ship._x;
		_y = _parent.ship._y - 30;
		if(life <= maxLife){ //回復
			life += 0.5;
			if(isDead && 100 <= life){
				isDead = false;
				launch();
			}
		}
	} else if(isInside){
		//船の中にいる
		//if(_parent.roloa.mode == 3){
		//	_visible = false;
		//} else {
		//	_visible = true;
		//}
		_visible = !_parent.ship.cover._visible;
		//_y = _parent.ship._y - 30;
		//_x = _parent.ship._x;
		vy += 0.5;

		//ロロア同様足を動かす
		Lleg._rotation *= 0.8;
		Rleg._rotation *= 0.8;
		var xsin =  (((_x >> 4) & 1) << 1) - 1;
		Lleg._rotation += xsin * (vx) * 4 + Math.abs(vy);
		Rleg._rotation -= xsin * (vx) * 4 + Math.abs(vy);
		//足の位置によって地面からの高さが変わる
		var ground = (Math.abs(Lleg._rotation) + Math.abs(Rleg._rotation)) * 0.1 + 30;
		if(_parent.ship._y <= _y + ground){
			_y = _parent.ship._y - ground;
			vy = Math.max(this._parent.ship.vy , 0);
			_x += _parent.ship.vx;
		}		
		

		
		Larm._rotation = (Rleg._rotation - 77) * 0.1 + Larm._rotation * 0.9;
		Rarm._rotation = (Rleg._rotation + 77) * 0.1 + Rarm._rotation * 0.9;
		//(Math.atan2(_ymouse,_xmouse) / Math.PI * 180 )
		//ロロアの方を向く
		if(vx < -0.5){
			_xscale = 100;
		} else if(0.5 < vx){
			_xscale = -100;
		}
	
		//適当に左右に歩く
		if(Math.random() < 0.005){
			tgtX = Math.random() * 50 - 80;
		}
		//壁
		if(_x < _parent.ship._x - 50 ){
			_x = _parent.ship._x - 50;
		}
		
		vx *= 0.5;
		vx += Math.min(Math.max( ( tgtX - (_x - _parent.ship._x) ) * 0.1, -0.3) , 0.3);;
	} else if(goInside || isDead) {
		vx = Math.min(Math.max((_parent.ship._x - _x) * 0.1,-moveSpeed),moveSpeed);
		vy = Math.min(Math.max((_parent.ship._y - _y) * 0.1,-moveSpeed),moveSpeed);
		_x += vx;
		_y += vy;
		if(vx * vx + vy * vy < 1){
			//船の中に入るときの動作
			goInside = false;
			isInside = true;
			isResting = true;
			_visible = false;
						_parent.ship.restingSatelite._visible = true; //休みサテライトと見た目入れ替わる
			Larm.gotoAndStop(2);
			Rarm.gotoAndStop(2);
			tgtX = 0;
		}
	} else if( goRoof ) {
		vx = Math.min(Math.max((_parent.ship._x -       _x) * 0.1,-moveSpeed),moveSpeed);
		vy = Math.min(Math.max((_parent.ship._y - 150 - _y) * 0.1,-moveSpeed),moveSpeed);
		_x += vx;
		_y += vy;
		if(vx * vx + vy * vy < 1){
			//船の屋上に乗るときの動作
			goRoof = false;
			isStayRoof = true;
			Larm.gotoAndStop(1); //いちおうパネル展開動作
			Rarm.gotoAndStop(1);
			Lleg._rotation = 0;
			Rleg._rotation = 0;
		}
	} else if(isStayRoof){
		vx = _parent.ship.vx;
		_x = _parent.ship._x;
		vy = _parent.ship.vy;
		_y = _parent.ship._y - 140;
		Larm_rot = -20;
		Rarm_rot =  20;
		//Larm_rot = _ymouse;
		if(_parent.ship.hateZako || _parent.enemy.isAlive){
			fighting();
		}
	} else {
		//外にいる時
		if(_parent.enemy.isAlive){ //ボス敵と戦闘中
			fighting();
			turnTo(_parent.enemy);
			
			
		} else if(hateZako._x) { //自身のターゲットを狙う
			fighting();
			turnTo(hateZako);
			tgtY = hateZako._y; //XXX
			//trace(hateZako._x + "",""+ hateZako);
			

		} else if(_parent.ship.hateZako._x){ //船のターゲットを自身のターゲットに
			hateZako = _parent.ship.hateZako;
		} else if(0 < beam_ammo){
			//ビーム残留対策・ビームを放ってる途中にボス・敵がいなくなってしまった場合
			_parent.satebeam._visible = false;
			beam_ammo = 0;
			Larm_rot = 10;
			Rarm_rot = -10;
		} else {
			tgtY = Math.min(Math.max( (700 - _parent.ship._y) ,100),600);
			//trace(tgtY);
		}
		//母船と対称の位置に動こうとする
		tgtX = Math.min(Math.max((centerX - _parent.ship._x)*2,100),600);
		vx = Math.min(Math.max((tgtX - _x) * 0.1,-moveSpeed),moveSpeed);
		vy = Math.min(Math.max((tgtY - _y) * 0.1,-moveSpeed),moveSpeed);
		
		
	}
	
}

