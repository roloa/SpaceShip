,"function resItemInit(){
	this.vx = 0;
	this.vy = 0;
	this.hTCnt = Math.random() * 5;
}

function resItemPhysical(){
	this._x += this.vx;
	this._y += this.vy;
	if(0.1 < Math.abs(this.vy)){
		this.vy *= 0.99;
	}
	if(-2 < this.vx){
		//this.vy *= 0.95;
		this.vx -= 0.1;
	}
	
	if(5 < this.hTCnt++){
		if(this._parent.roloa.hitCoin(this._x,this._y)){
			this.got();
			this.removeMovieClip();
		}
		if(this._parent.satelite.hitCoin(this._x,this._y)){
			this.got();
			this.removeMovieClip();
		}
		if(this._parent.ship.hitTest(this._x,this._y,true)){
			this.got();
			this.removeMovieClip();
		}
		this.hTCnt = 0;
	}
	if(	this._x < -100 || 1380 < this._x ||
		this._y < -100 || 740 < this._y){
		this.removeMovieClip();		
	}
