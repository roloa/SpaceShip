1,1,False,False,True,,"/**
	アップグレードについて
	
	パワー
		攻撃力を上げる。
	ライフ
		ライフ容量を増やす。
	リロード
		リロード速度を早める。
	マガジン
		１回のリロードで撃てる弾が増える。
	リペア
		回復速度を早める。
		シップに使うと全回復する。
	
*/

//

//0:リペア 1:ライフ 2:パワー 3:リロード 4:マガジン

//種類をランダムに決める
kind = Math.floor(Math.random() * 5 + 1 );
gotoAndStop(kind);

vx = Math.random() * 10 - 5;
vy = Math.random() * 10 - 5;

tgtX = 400 + Math.random() * 200;
tgtY = 500 + Math.random() * 100;

state = 1;
isDrag = false;
function onEnterFrame(){
	//
	if(state == 1){
		//パーティクルとしてふるまう
		_x += vx;
		_y += vy;
		vx *= 0.95;
		vy *= 0.95;
		if(vx * vx + vy * vy < 1){
			state = 2;
		}
	} else if(state == 2){
		//アイテム欄的なところにおさまる
		if(isDrag){
			_x = _parent._xmouse;
			_y = _parent._ymouse;
		} else {
			_x = _x * 0.9 + tgtX * 0.1;
			_y = _y * 0.9 + tgtY * 0.1;
		}
	}
}

function onPress(){
	isDrag = true;
}
function onMouseUp(){
	if(isDrag){
		isDrag = false;
		//HUDのボタン類との当たり判定をとる
		_parent.HUD.dropUpgrade(this);
	}
	
