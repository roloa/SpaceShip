,"//

function disable(){
	this._visible = false;
	this.isEnable = false;
	if(this.user != null){
		this.user.releaseGoods();
	}
}

function ON_OFF(){
	if(this.isEnable){
		this.disable();
	} else {
		//this._visible = true;
		this.isEnable = true;
		if(this._parent.roloa.mode == 1){
			this._x = this._parent.ship._x - 50;
			this._y = this._parent.ship._y - 50;
			this.isOutside = false;
			this._visible = true;
		} else {
			this._x = this._parent.ship._x - 50;
			this._y = this._parent.ship._y - 150;
			this.isOutside = true;
			this._visible = true;
		}
	}
}

function getCost(){
	if(	this.level == 0){
		return 10;
	} else {
		return this.level * 5;
	}
}

function goodsPhisicalInit(){
	this.isOutside = false;
	this.vx = 0;
	this.vy = 0;
	this.user = null;
	this.isEnable = false;
	this._visible = false;
	
	this.level = 0;
}

function isNearItem(target){

	if(this.isEnable == false){
		return false;
	}
	if(this.user != null){
		return false;
	}
	if(Math.abs(target._x - this._x)  < 30){
		if(Math.abs(target._y - this._y - 30)  < 50)
		return true;
	}
	return false;
}

function goodsPhisical(){

	if(this.isEnable){
			
		if(this.user != null){
			
		} else {
			this.vy += 0.3;
			this._x += this.vx;
			this._y += this.vy;
			this._rotation *= 0.9;
			if(this.isOutside){
				//外にいる場合の地面
				if(this._parent.ship._y - 110 <= this._y + this.legHeight ){
					if(this._parent.ship._x - 120 < this._x && this._x < this._parent.ship._x + 180){
						this.vx = this._parent.ship.vx;
						this.vy = Math.max(this._parent.ship.vy , 0);
						//this._x += this._parent.ship.vx;
						this._y = this._parent.ship._y - this.legHeight - 110;
					} else if(1000 < this._y){
						this.disable();
						
					}
				}
			} else {
				//中にある時の処理
				this._visible = !this._parent.ship.cover._visible;
				
				if(this._parent.ship._y <= this._y + this.legHeight ){
					this.vx = this._parent.ship.vx;
					this.vy = Math.max(this._parent.ship.vy , 0);
					//this._x += this._parent.ship.vx ;
					this._y = this._parent.ship._y - this.legHeight;
				}
				if(this._x < this._parent.ship._x - 70 ){
					this._x = this._parent.ship._x - 70;
				} else if(this._parent.ship._x + 150 < this._x){
					this._x = this._parent.ship._x + 150;
				}
			}

		}
	}
}"
