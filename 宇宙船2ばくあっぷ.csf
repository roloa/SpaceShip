			vx += obj.vx * 0.2;
			vy += obj.vy * 0.2;
			vx = Math.min(Math.max(vx,-5),5);
			vy = Math.min(Math.max(vy,-5),5);
			
			life -= obj.power;
			if(isDead <= 0 && life < 0){
				dead();
			}
		}
	}
	function spawn(lvl){
		_visible = true;
		isAlive = true;
		isDead = false;
		life = lvl * 5;
		power = lvl;
		maxCoolTime = 100 * (100 / (100+lvl));
		
		
		count = 0;
		//reloadTime = 1;
		//power = 0.1;
		
		dropItemNum = Math.floor(lvl / 8 + 1);
		
		tgtX = 1000;
		tgtY = 320;
		
		//レベル表示を変える
		_parent.HUD.enemyLevel.text = ""Lv "" + level ;
				
		//音楽を変える
		_root.music.battle();
	}
	function dead(){
		//TODO 大爆発パーティクルを残す
		isDead = 100;
	}
	function disable(){
		_x = 1500;
		_visible = false;
		isAlive = false;
		isDead = false;
		spawnCount = 1000;
		
		//音楽を変える
		_root.music.explore();
	}
	function win(){ //というか撤退
		tgtX = 1000;
		isWinning = true;
	}
	function winning(){
		tgtX = 1500;
		tgtY = 320;
		if(1400 < _x){
			isWinning = false;
			disable();
		}
	}
	
	disable();
}

