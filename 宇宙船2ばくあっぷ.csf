1,1,False,False,True,,"/*
	ザコ敵はそれ自身が弾としての性質を持ち、
	シールドやミサイルなどの味方オブジェクトにぶつかると、
	耐久を削ったりする
*/

isHitObj = true;
vx = -2;
vy = Math.random() * 2 - 1;
isEnemy = true;

//何かにぶつかられた時、ぶつかってきた相手が引数になって呼ばれる
function onHit(oppo){
	
	//ぶつかられた相手が当たり判定を持つ場合
	if(oppo.isHitObj){
		if(oppo.vx) vx += oppo.vx;
		if(oppo.vy) vy += oppo.vy;
	} else {
		//当たり判定を持たない物に撃たれた場合
		//相手のパワーの分ライフが減って、なくなった場合は爆発四散
		//
		vx += 1;
		vy = -vy;
		if(oppo.vx) vx += oppo.vx;
		if(oppo.vy) vy += oppo.vy;

		life -= oppo.power;
		if(life <= 0){
			_parent.attach(""bomb"",{_x:_x,_y:_y});
			_parent.attach(""coin"",{_x:_x,_y:_y}); //攻撃で倒した場合はアイテムをドロップ
			_parent.attach(""coin"",{_x:_x,_y:_y});
			_parent.attach(""coin"",{_x:_x,_y:_y});
			if(Math.random() < 0.1){_parent.attach(""missileBox"" , {_x:_x,_y:_y});}
			
			_parent.remove(this,isEnemy);
		}
	}
}

function onEnterFrame(){
	_x += vx;
	_y += vy;

	vy += Math.sin( _x * 0.05) * 0.1;
	if(-2 < vx ) vx -= 0.1;

	//攻撃当たり判定
	var hitter = _parent.hitCheck(_x,_y,isEnemy);
	if(hitter){
		vx += 1;
		vy = -vy;
		if(hitter.vx) vx += hitter.vx;
		if(hitter.vy) vy += hitter.vy;
		//_parent.remove(this,isEnemy);
		hitter.onHit(this);
	}

	//画面外ロスト判定
	//敵なのでちょっと広い
	if(	this._x < 0 || 1400 < this._x ||
		this._y < -100 || 740 < this._y){
		_parent.remove(this,isEnemy);
	} else if(Math.random() < 0.1){
		_parent.ship.setHate(this);
	}
}",-1,0,0,,,
