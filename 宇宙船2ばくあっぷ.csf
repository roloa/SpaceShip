null,null,null,
_root.main.mortar,
_root.main.IonBlast,
_root.main.LAW,
_root.main.dragonova,
_root.main.tate,
_root.main.recorder,
_root.main.boomerang,
_root.main.rapier
];

function pressButton(num){
	selectedNum = num + buttonScroll;
	reloadDisp();
}

function ON_OFF(){
	weapons[selectedNum].ON_OFF();
}

function lvup(){
	if( _root.main.ship.useMetal( weapons[selectedNum].getCost() )){
		weapons[selectedNum].level += 1;
	}
	reloadDisp();
}

//ボタン表示を更新する
function reloadDisp(){
	//アイテム表示
	for(var i=0 ; i < 6 ; i++){
		buttons[i].gotoAndStop( i + buttonScroll );
	}
	//オンオフ表示
	
	
	//選択枠表示
	var cursorNum = selectedNum - buttonScroll;
	if(0 <= cursorNum && cursorNum < 6){
		cursor._x = buttons[cursorNum]._x;
		cursor._y = buttons[cursorNum]._y;
		cursor._visible = true;
	} else {
		//cursor._alpha = 50;
		cursor._visible = false;
	}
	
	//テキスト類
	weaponName.text = weapons[selectedNum ].name;
	setsumei.text = weapons[selectedNum   ].setsumei;
	level.text = weapons[selectedNum      ].level;
	cost.text = weapons[selectedNum       ].getCost();

}

