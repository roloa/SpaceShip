	prevRestTime = 0;
	
	
/**
	オブジェクトの作成を行う。
	name : 作るオブジェクトの書き出し識別子
	obj  : 初期化用オブジェクト
	isHitobj : 当たり判定を持つオブジェクトかどうか
	isEnemy : 所属勢力
*/
function attach(name,obj,isHitobj,isEnemy){

	if(isHitobj){
		if(isEnemy){
			if(enemyNum < MAX_OBJ){
				var sindo = this.getNextHighestDepth();
				var newobj = this.attachMovie(name,name + sindo,sindo,obj);
				enemyNum += 1;
				do {
					enemyIndex++; 
					if(MAX_OBJ <= enemyIndex){
						enemyIndex = 0;
					}
				} while(enemys[enemyIndex] != null);
				enemys[enemyIndex] = newobj;
				newobj.registNum = enemyIndex;
			} else { trace(""enemyNum is Over!""); }
		} else {
			if(allyNum < MAX_OBJ){
				var sindo = this.getNextHighestDepth();
				var newobj = this.attachMovie(name,name + sindo,sindo,obj);
				allyNum += 1;
				do {
					allyIndex++; 
					if(MAX_OBJ <= allyIndex){
						allyIndex = 0;
					}
				} while(allys[allyIndex] != null);
				allys[allyIndex] = newobj;
				newobj.registNum = allyIndex;
				//trace(""ally regist at "" + allyIndex);
			} else { trace(""allyNum is Over!""); }
		}
	} else {
		var sindo = this.getNextHighestDepth();
		this.attachMovie(name,name + sindo,sindo,obj);
	}
}
