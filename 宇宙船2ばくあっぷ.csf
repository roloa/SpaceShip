				var dx = Math.cos(rad) * 200;
				var dy = Math.sin(rad) * 200;
				var rnd = Math.random();
				var x = pt.x + dx * rnd;
				var y = pt.y + dy * rnd;
				for(var i=0 ; i <= 7 ; i++){
					var hit = _parent.hitCheck(x,y);
					if(hit){
						hit.onHit({vx:0,vy:0,power:power/10});
						_parent.attach(""spark"",{_x:x,_y:y});
					}
					x += dx;
					y += dy;
				}
				
				if(--beam_ammo <= 0){
					//終了処理
					_parent.satebeam._visible = false;
					Larm_rot = 10;
					Rarm_rot = -10;
				}
