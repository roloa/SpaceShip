	//当たり判定リストに登録する。
	//登録オブジェクトの数がMAX_OBJを超えた場合は無視する

	//登録を解除する。
	function remove(mc,isEnemy){
		if(isEnemy){
			enemyNum -= 1;
			enemys[mc.registNum] = null;
		} else {
			allyNum -= 1;
			allys[mc.registNum] = null;
		}
		mc.removeMovieClip();
	}
	
	//指定座標にある敵対飛翔物を返す。
	//静的に配置されているものもチェックする。
	//isEnemyのところは、自分の属性を指定する。
	function hitCheck(x,y,isEnemy){
		if(isEnemy){
			if( shield.hitTest(x,y,true)) return shield;
			if( ship.hitTest(x,y,true)) return ship;
			if( satelite.hitTest(x,y,true)) return satelite;
			if( roloa.hitTest(x,y,true)) return roloa;
			if( tate.myHit(x,y)) return tate;
			for(var i=0 ; i < MAX_OBJ ; i++){
				if(allys[i].hitTest(x,y,true)){
					return allys[i];
				}
			}
		} else {
			if( enemy.hitTest(x,y,true)) return enemy;
			
			for(var i=0 ; i < MAX_OBJ ; i++){
				if(enemys[i].hitTest(x,y,true)){
					return enemys[i];
				}
			}
		}
		return null;
	}
	
